<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="httpApi" type="App.HttpApi.HttpApiConfig, App.HttpApi"/>
  </configSections>
  <httpApi authIPs="" errorResponse="DataResult" jsonEnumFormatting="Text" wrap="" jsonIndented="Indented" jsonDateTimeFormat="yyyy-MM-dd"/>
  <!--
    有关 web.config 更改的说明，请参见 http://go.microsoft.com/fwlink/?LinkId=235367。

    可在 <httpRuntime> 标记上设置以下特性。
      <system.Web>
        <httpRuntime targetFramework="4.5" />
      </system.Web>
  -->
  <system.web>
    <compilation debug="true" targetFramework="4.5"/>
    <!--
    <httpHandlers>
      <add verb="*" path="HttpApi*.axd" type="App.HttpApi.HttpApiHandler, App.HttpApi"/>
    </httpHandlers>
    -->
    <pages controlRenderingCompatibilityVersion="4.0"/>
  </system.web>
  <system.webServer>
    <modules>
      <add name="HttpApiModule" type="App.HttpApi.HttpApiModule" />
    </modules>
    <handlers>
      <!--add name="HttpApi" verb="*" path="HttpApi*.axd" type="App.HttpApi.HttpApiHandler, App.HttpApi"/-->
    </handlers>
  </system.webServer>
</configuration>